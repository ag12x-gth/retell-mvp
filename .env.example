# Database
DATABASE_URL="file:./prisma/dev.db"
NODE_ENV="development"
PORT=3000

# Twilio
TWILIO_ACCOUNT_SID="your_twilio_account_sid_here"
TWILIO_AUTH_TOKEN="your_twilio_auth_token_here"
TWILIO_PHONE_NUMBER="your_twilio_phone_number_here"
TWILIO_API_KEY="your_twilio_api_key_here"
TWILIO_API_SECRET="your_twilio_api_secret_here"

# Retell.ai
RETELL_API_KEY="your_retell_api_key_here"
RETELL_WORKSPACE_ID="your_retell_workspace_id_here"

# OpenAI
OPENAI_API_KEY="your_openai_api_key_here"

# JWT Secret (gere com: openssl rand -hex 32)
JWT_SECRET="your_random_jwt_secret_32_characters_minimum"

# Webhook URLs (serﾃ｡ preenchido automaticamente no Replit)
WEBHOOK_BASE_URL="http://localhost:3000"

